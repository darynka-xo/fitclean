{
  "name": "fitclean",
  "version": "1.0.0",
  "description": "Complete laundry management system for fitness clubs",
  "scripts": {
    "start": "./start.sh",
    "dev": "./start.sh",
    "frontend": "cd frontend && npm run dev",
    "frontend:install": "cd frontend && npm install",
    "api": "cd api && python -m uvicorn app.main:app --reload --host 0.0.0.0 --port 8000",
    "api:install": "cd api && pip install -r requirements.txt",
    "supabase": "cd supabase && supabase start",
    "supabase:stop": "cd supabase && supabase stop",
    "supabase:reset": "cd supabase && supabase db reset",
    "types": "cd supabase && supabase gen types typescript --local --schema public > ../frontend/src/types/supabase.ts",
    "build": "cd frontend && npm run build",
    "deploy": "cd supabase && supabase functions deploy telegram-bot"
  },
  "keywords": ["laundry", "fitness", "telegram-bot", "nextjs", "supabase", "fastapi"],
  "author": "FitClean Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "."
  }
}
