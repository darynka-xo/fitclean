# Supabase
VITE_SUPABASE_URL=https://your-project.supabase.co
VITE_SUPABASE_ANON_KEY=your_anon_key

# Kaspi QR API
VITE_KASPI_MERCHANT_ID=your_merchant_id
VITE_KASPI_TERMINAL_ID=your_terminal_id
VITE_KASPI_API_KEY=your_api_key

# WhatsApp API (GreenAPI or similar)
VITE_WHATSAPP_API_URL=https://api.green-api.com
VITE_WHATSAPP_INSTANCE_ID=your_instance_id
VITE_WHATSAPP_API_TOKEN=your_token

# Locker Server (runs locally on tablet)
# These are used by the server, not the web app
LOCKER_PORT=8080
SERIAL_PORT=/dev/ttyUSB0
USE_MOCK=false
